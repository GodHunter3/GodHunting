# Class template
class Solution:
# function to revese
    def reverseVowels(self, s: str) -> str:
    # algorithm
        s=list(s)
        x= len(s)-1
        i=0
        # loops
        while x>i:
            if s[i] in 'aeiouAEIOU':
                if s[x] in 'aeiouAEIOU':
                    s[i],s[x]=s[x],s[i]
                    i+=1
                x-=1
            else:
                i+=1
        # join and return
        return "".join(s)
            
